kind: Service
apiVersion: v1
metadata:
  labels:
    app: {{ iot_ui_application }}
    app.kubernetes.io/component: {{ iot_ui_application }}
    app.kubernetes.io/instance: {{ iot_ui_application }}
  name: {{ iot_ui_application }}
spec:
  ports:
  - name: 8080-tcp
    port: 8080
    protocol: TCP
    targetPort: 8080
  selector:
    deployment: {{ iot_ui_application }}
  sessionAffinity: None
  type: ClusterIP
